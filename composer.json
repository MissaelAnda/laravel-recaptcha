{
    "name": "missael-anda/laravel-recaptcha",
    "description": "A simple ReCaptcha wrapper for Laravel.",
    "keywords": [
        "laravel",
        "recaptcha",
        "google"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MissaelAnda\\ReCaptcha\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Missael H. Anda",
            "email": "missael.anda@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0 || ^10.0",
        "illuminate/validation": "^9.0 || ^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MissaelAnda\\ReCaptcha\\ReCaptchaServiceProvider"
            ],
            "aliases": {
                "ReCaptcha": "MissaelAnda\\ReCaptcha\\Facade\\ReCaptcha"
            }
        }
    }
}